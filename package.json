{
  "name": "catalog-exercise",
  "version": "1.0.0",
  "description": "This is code exercise for my beginning at map colonies team",
  "main": "./src/index.ts",
  "scripts": {
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "prelint:fix": "npm run format:fix",
    "prelint": "npm run format",
    "lint:openapi": "openapi lint ./swagger.yaml",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prebuild": "npm run clean",
    "build": "tsc --project tsconfig.build.json && npm run assets:copy",
    "start": "npm run build && cd dist && node ./index.js",
    "dev": "npm run build && cd dist && nodemon ./index.js",
    "assets:copy": "copyfiles -f ./config/* ./dist/config && copyfiles -f ./swagger.yaml ./dist/ && copyfiles ./package.json dist && copyfiles -f ./config/.env ./dist/config",
    "clean": "rimraf dist",
    "test:unit": "DOTENV_CONFIG_PATH=./config/.env.test jest --config=./tests/configurations/unit/jest.config.js",
    "test:unit:windows": "jest --config=./tests/configurations/unit/jest.config.js",
    "test": "npm run test:unit:windows",
    "integ": "jest --config=./tests/configurations/integration/jest.config.js"
  },
  "directories": {
    "test": "tests"
  },
  "author": "Ron Nachum",
  "license": "ISC",
  "dependencies": {
    "@map-colonies/js-logger": "^0.0.5",
    "@map-colonies/read-pkg": "0.0.1",
    "@turf/boolean-contains": "^6.5.0",
    "@turf/intersect": "^6.5.0",
    "@turf/turf": "^6.5.0",
    "@types/geojson": "^7946.0.10",
    "compression": "^1.7.4",
    "config": "^3.3.7",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "express-openapi": "^12.1.0",
    "express-openapi-validator": "^4.13.8",
    "http-status-codes": "^2.2.0",
    "pg": "^8.8.0",
    "pgtools": "^0.1.1",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.6.2",
    "tsyringe": "^4.7.0",
    "typeorm": "^0.3.10",
    "uuid": "^9.0.0",
    "wkx": "^0.5.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.1",
    "@commitlint/config-conventional": "^17.0.0",
    "@map-colonies/eslint-config": "^3.1.1",
    "@map-colonies/prettier-config": "0.0.1",
    "@map-colonies/standard-version-update-helm-version": "^2.0.1",
    "@map-colonies/standard-version-update-openapi3-version": "0.0.2",
    "@redocly/openapi-cli": "^1.0.0-beta.94",
    "@types/compression": "^1.7.2",
    "@types/config": "^3.3.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.7",
    "@types/multer": "^1.4.7",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "@types/yamljs": "^0.2.31",
    "commitlint": "^17.0.1",
    "copyfiles": "^2.4.1",
    "eslint": "^8.22.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-create-mock-instance": "^2.0.0",
    "jest-html-reporters": "^3.0.8",
    "jest-openapi": "^0.14.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "standard-version": "^9.5.0",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.8",
    "typescript": "^4.6.3"
  }
}